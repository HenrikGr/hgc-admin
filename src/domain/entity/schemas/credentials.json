{
  "$id": "http://hgc.se/credentials.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["username", "password"],
  "additionalProperties": false,
  "properties": {
    "username": {
      "title": "User name",
      "type": "string",
      "default": "",
      "minLength": 4
    },
    "password": {
      "title": "Password",
      "type": "string",
      "default": "",
      "isPassword": true
    },
    "client_id": {
      "title": "Client ID",
      "type": "string"
    },
    "grant_type": {
      "title": "Grant Type",
      "type": "string",
      "default": "password"
    }
  }
}
