{
  "$id": "http://hgc.se/profile.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "profile",
  "type": "object",
  "required": ["firstName","lastName","email","phone"],
  "additionalProperties": false,
  "properties": {
    "firstName": {
      "title": "First name",
      "type": "string",
      "default": "",
      "isNotEmpty": true
    },
    "lastName": {
      "title": "Last name",
      "type": "string",
      "default": "",
      "isNotEmpty": true
    },
    "email": {
      "title": "Email",
      "type": "string",
      "default": "",
      "isNotEmpty": true
    },
    "phone": {
      "title": "Phone",
      "type": "string",
      "default": ""
    }
  }
}
