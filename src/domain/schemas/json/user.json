    },
    "admin": {
      "$id": "/properties/admin",
      "title": "The Admin Schema",
      "default": false,
      "type": "boolean"
    },
    "active": {
      "$id": "/properties/active",
      "title": "The Active Schema",
      "default": false,
      "type": "boolean"
    }
