{
  "$id": "http://hgc.se/profile.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["firstName","lastName","email","phone"],
  "additionalProperties": false,
  "properties": {
    "_id": {
      "$id": "/properties/_id",
      "title": "id of profile",
      "type": "string",
      "readOnly": true
    },
    "firstName": {
      "$id": "/properties/firstName",
      "title": "First name",
      "type": "string",
      "default": ""
    },
    "lastName": {
      "$id": "/properties/lastName",
      "title": "Last name",
      "type": "string",
      "default": ""
    },
    "email": {
      "$id": "/properties/email",
      "title": "Email",
      "type": "string",
      "default": ""
    },
    "phone": {
      "$id": "/properties/phone",
      "title": "Phone",
      "type": "string",
      "default": ""
    },
    "updatedAt": {
      "$id": "/properties/updatedAt",
      "title": "Updated",
      "type": "string",
      "readOnly": true
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "title": "Updated",
      "type": "string",
      "readOnly": true
    }
  }
}