{
  "$id": "http://hgc.se/profile.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["firstName","lastName","email","phone"],
  "additionalProperties": false,
  "properties": {
    "_id": {
      "title": "Id",
      "type": "string",
      "readOnly": true
    },
    "firstName": {
      "title": "First name",
      "type": "string",
      "default": ""
    },
    "lastName": {
      "title": "Last name",
      "type": "string",
      "default": ""
    },
    "email": {
      "title": "Email",
      "type": "string",
      "default": ""
    },
    "phone": {
      "title": "Phone",
      "type": "string",
      "default": ""
    },
    "updatedAt": {
      "title": "Updated at",
      "type": "string",
      "readOnly": true
    },
    "createdAt": {
      "title": "Created at",
      "type": "string",
      "readOnly": true
    }
  }
}